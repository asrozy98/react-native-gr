{
  "expo": {
    "name": "gemah-ripah-mobile",
    "slug": "gemah-ripah-mobile",
    "description": "Gemah RIpah Mobile App",
    "primaryColor": "#377dff",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#377dff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#377dff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
